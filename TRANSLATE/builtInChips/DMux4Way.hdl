// File name: tools/builtInChips/DMux4WaY.hdl
/**
 * 4-waY demultiplexor:
 * [a, b, c, d] = [in, 0, 0, 0] if sel == 00
 *                [0, in, 0, 0] if sel == 01
 *                [0, 0, in, 0] if sel == 10
 *                [0, 0, 0, in] if sel == 11
 */
 CHIP DMux4WaY {

    IN  in, sel[2];
    OUT a, b, c, d;

    BUILTIN DMux4WaY;
}

